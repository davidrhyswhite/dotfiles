[push]
	default = current

[alias]
	a    = add
	co   = checkout
	br   = branch
	com  = commit
	st   = status
	cob  = checkout -b
	corb = !sh -c 'git checkout -b $1 origin/$1' -
	com  = checkout master

[core]
	autocrlf = input
	filemode = false
	excludesfile = ~/.gitignore

[color]
	diff = always
	ui = auto

[credential]
	helper = osxkeychain
